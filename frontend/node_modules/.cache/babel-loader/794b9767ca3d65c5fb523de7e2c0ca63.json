LA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,WATA;AAUA,SAdA;AAeA,OAlBA,WAmBA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,EAAA;AACA;AACA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,MAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,OAAA;AACA,SANA,MAMA,IAAA,KAAA,CAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,SALA,MAKA;AACA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA,CAAA,OAAA;AACA;AACA,OAnCA;AAoCA;AArDA,GATA;AAgEA,EAAA,OAhEA,qBAgEA;AACA,QAAA,EAAA,GAAA,IAAA;AACA,IAAA,KAAA,CACA,GADA,CACA,uCAAA,EAAA,CAAA,EAAA,GAAA,UADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,MAAA,EAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AACA,KAJA,WAKA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,EAAA;AACA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,MAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,OAAA;AACA,OANA,MAMA,IAAA,KAAA,CAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,OALA,MAKA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA,CAAA,OAAA;AACA;AACA,KArBA;AAsBA;AAxFA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-row v-for=\"(answer, index) in answers\" :key=\"index\">\n      <v-col cols=\"12\" class=\"d-flex justify-center\">\n        <v-card v-if=\"answer.correct\" color=\"#f6febd80\" class=\"mt-2 answer\">\n          <v-card-title class=\"headline\">\n            {{ answer.user.username }}</v-card-title\n          >\n\n          <v-card-text>{{ answer.text }}</v-card-text>\n\n          <v-card-actions class=\"card-actions\">\n            <v-btn\n              v-if=\"\n                check_cookie_value('id') == answer.query.user.id &&\n                !answer.correct\n              \"\n              @click=\"corrAnswer(answer.id, answer.user.id)\"\n              text\n            >\n              Select as correct</v-btn\n            >\n            <v-btn v-if=\"answer.correct\" disabled text>\n              Marked as correct.</v-btn\n            >\n          </v-card-actions>\n          <v-card-actions class=\"card-actions2\">\n            <v-btn text disabled> {{ convertDate(answer.date) }} </v-btn>\n          </v-card-actions>\n        </v-card>\n        <v-card v-else color=\"#063ee50a\" class=\"mt-2 answer\">\n          <v-card-title class=\"headline\">\n            {{ answer.user.username }}</v-card-title\n          >\n\n          <v-card-text>{{ answer.text }}</v-card-text>\n\n          <v-card-actions class=\"card-actions\">\n            <v-btn\n              v-if=\"\n                check_cookie_value('id') == answer.query.user.id &&\n                !answer.correct\n              \"\n              @click=\"corrAnswer(answer.id, answer.user.id)\"\n              text\n            >\n              Select as correct</v-btn\n            >\n            <v-btn v-if=\"answer.correct\" disabled text>\n              Marked as correct.</v-btn\n            >\n          </v-card-actions>\n          <v-card-actions class=\"card-actions2\">\n            <v-btn text disabled> {{ convertDate(answer.date) }} </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"Answers\",\n  components: {},\n  props: [\"id\"],\n  data() {\n    return {\n      answers: [],\n    };\n  },\n  methods: {\n    convertDate(dateString) {\n      dateString = dateString.toString();\n      return dateString.replace(/(\\d{4})(\\d\\d)(\\d\\d)/g, \"$2/$3/$1\");\n    },\n    check_cookie_value(name) {\n      var match = document.cookie.match(\n        new RegExp(\"(^| )\" + name + \"=([^;]+)\")\n      );\n      if (match) {\n        return match[2];\n      } else {\n        return -1;\n      }\n    },\n    corrAnswer(idAns, idUser) {\n\n      axios\n        .put(\"http://localhost:8085/api/answers/correct/\" + idAns)\n        .then(function () {\n          axios\n            .get(\"http://localhost:8085/api/utenti/alt/\" + idUser)\n            .then(function (response) {\n              var money = response.data.credits +25;\n              axios\n                .put(\n                  \"http://localhost:8085/api/utenti/\" +\n                    idUser +\n                    \"/updatecredit\",\n                  { credits: money }\n                )\n                .then((response) => {\n                  console.log(response.data.credits);\n                });\n            });\n        })\n        .catch((error) => {\n          if (error.response) {\n            // The request was made and the server responded with a status code\n            // that falls out of the range of 2xx\n            console.log(error.response.data);\n            console.log(error.response.status);\n            console.log(error.response.headers);\n          } else if (error.request) {\n            // The request was made but no response was received\n            // `error.request` is an instance of XMLHttpRequest in the browser and an instance of\n            // http.ClientRequest in node.js\n            console.log(error.request);\n          } else {\n            // Something happened in setting up the request that triggered an Error\n            console.log(\"Errorripdd\", error.message);\n          }\n        });\n    },\n  },\n  mounted() {\n    var vm = this;\n    axios\n      .get(\"http://localhost:8085/api/queries/\" + vm.id + \"/answers\")\n      .then(function (response) {\n        vm.answers = response.data;\n      })\n      .catch((error) => {\n        if (error.response) {\n          // The request was made and the server responded with a status code\n          // that falls out of the range of 2xx\n          console.log(error.response.data);\n          console.log(error.response.status);\n          console.log(error.response.headers);\n        } else if (error.request) {\n          // The request was made but no response was received\n          // `error.request` is an instance of XMLHttpRequest in the browser and an instance of\n          // http.ClientRequest in node.js\n          console.log(error.request);\n        } else {\n          // Something happened in setting up the request that triggered an Error\n          console.log(\"Errorripdd\", error.message);\n        }\n      });\n  },\n};\n</script>\n<style scoped>\n.answer {\n  height: 30vh;\n  width: 70vw;\n  margin-left: auto;\n  margin-right: auto;\n}\n.card-actions {\n  position: absolute;\n  bottom: 0;\n}\n.card-actions2 {\n  position: absolute;\n  bottom: 0;\n  right: 0;\n}\n</style>\n\n"],"sourceRoot":"src/components"}]}