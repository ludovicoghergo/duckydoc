AA;AACA,OANA,MAMA,IAAA,KAAA,CAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,OALA,MAKA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA,CAAA,OAAA;AACA;AACA,KArBA;AAsBA;AA7CA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-row v-for=\"(answer, index) in answers\" :key=\"index\">\n      <v-col cols=\"12\" class=\"d-flex justify-center\">\n        <v-card color=\"#f4ecb4\" class=\"mt-2 answer\">\n          <v-card-title class=\"headline\">\n            {{ answer.user.username }}</v-card-title\n          >\n\n          <v-card-text>{{ answer.text }}</v-card-text>\n\n          <v-card-actions class=\"card-actions\">\n            <v-btn v-if=\"check_cookie_value('id') == answer.query.user.id\" text>\n              Select as correct</v-btn\n            >\n          </v-card-actions>\n        </v-card>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"Answers\",\n  components: {},\n  props: [\"id\"],\n  data() {\n    return {\n      answers: [],\n    };\n  },\n  methods: {\n    check_cookie_value(name) {\n      var match = document.cookie.match(\n        new RegExp(\"(^| )\" + name + \"=([^;]+)\")\n      );\n      if (match) {\n        return match[2];\n      } else {\n        return -1;\n      }\n    },\n  },\n  mounted() {\n    var vm = this;\n    axios\n      .get(\"http://localhost:8082/api/queries/\" + vm.id + \"/answers\")\n      .then(function (response) {\n        vm.answers = response.data;\n      })\n      .catch((error) => {\n        if (error.response) {\n          // The request was made and the server responded with a status code\n          // that falls out of the range of 2xx\n          console.log(error.response.data);\n          console.log(error.response.status);\n          console.log(error.response.headers);\n        } else if (error.request) {\n          // The request was made but no response was received\n          // `error.request` is an instance of XMLHttpRequest in the browser and an instance of\n          // http.ClientRequest in node.js\n          console.log(error.request);\n        } else {\n          // Something happened in setting up the request that triggered an Error\n          console.log(\"Errorripdd\", error.message);\n        }\n      });\n  },\n};\n</script>\n<style scoped>\n.answer {\n  height: 30vh;\n  width: 70vw;\n  margin-left: auto;\n  margin-right: auto;\n}\n.card-actions {\n  position: absolute;\n  bottom: 0;\n}\n</style>\n\n"],"sourceRoot":"src/components"}]}