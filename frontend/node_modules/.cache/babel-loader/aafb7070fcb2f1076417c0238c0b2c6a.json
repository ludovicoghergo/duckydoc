KAAA,QAAA,CAAA,EADA;AAEA,UAAA,IAAA,EAAA;AACA,YAAA,EAAA,EAAA,KAAA,QAAA,CAAA,IAAA,CAAA,EADA;AAEA,YAAA,QAAA,EAAA,KAAA,QAAA,CAAA,IAAA,CAAA;AAFA,WAFA;AAMA,UAAA,IAAA,EAAA,KAAA,QAAA,CAAA,IANA;AAOA,UAAA,IAAA,EAAA,KAAA,QAAA,CAAA,IAPA;AAQA,UAAA,OAAA,EAAA,KAAA,QAAA,CAAA;AARA,SAJA;AAcA,QAAA,IAAA,EAAA;AAdA,OADA,EAiBA,IAjBA,CAiBA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,OAnBA,WAoBA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,EAAA;AACA;AACA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,MAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,OAAA;AACA,SANA,MAMA,IAAA,KAAA,CAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,SALA,MAKA;AACA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA,KAAA,CAAA,OAAA;AACA;AACA,OApCA;AAqCA;AArDA,GAHA;AA0DA,EAAA,IA1DA,kBA0DA;AACA,WAAA;AACA,MAAA,SAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EADA;AAEA,MAAA,QAAA,EAAA,EAFA;AAGA,MAAA,MAAA,EAAA,EAHA;AAIA,MAAA,MAAA,EAAA,KAJA;AAKA,MAAA,UAAA,EAAA;AALA,KAAA;AAOA,GAlEA;AAmEA,EAAA,OAnEA,qBAmEA;AACA,QAAA,EAAA,GAAA,IAAA;AACA,IAAA,KAAA,CACA,GADA,CACA,uCAAA,EAAA,CAAA,SADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,MAAA,EAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,MAAA,EAAA,CAAA,MAAA,GAAA,EAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA;AACA,KALA,WAMA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,EAAA;AACA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,MAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,OAAA;AACA,OANA,MAMA,IAAA,KAAA,CAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,OALA,MAKA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA,KAAA,CAAA,OAAA;AACA;AACA,KAtBA;AAuBA;AA5FA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-card elevation=\"2\" outlined color=\"#faf6d6\" class=\"mt-2 Question\">\n      <v-card-title> {{ author }}</v-card-title>\n\n      <v-card-text>{{ question.text }} </v-card-text>\n      <v-card-actions class=\"card-actions\">\n        <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn color=\"primary\" dark v-bind=\"attrs\" v-on=\"on\">\n              Answer\n            </v-btn>\n          </template>\n          <v-card>\n            <v-card-title>\n              <span class=\"headline\" v-if=\"question.user != undefined\"\n                >Answer to {{ question.user.username }}</span\n              >\n            </v-card-title>\n            <v-card-text>\n              <v-container>\n                <v-row>\n                  <v-col>\n                    <v-textarea\n                      v-model=\"answer_txt\"\n                      label=\"Write here your answer\"\n                      outlined\n                      required\n                    ></v-textarea>\n                  </v-col>\n                </v-row>\n              </v-container>\n              <small>*Please respect the rules</small>\n            </v-card-text>\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\n                Close\n              </v-btn>\n              <v-btn\n                color=\"blue darken-1\"\n                text\n                @click=\"(dialog = false), answerQuestion()\"\n              >\n                Send\n              </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n      </v-card-actions>\n    </v-card>\n    <Answers :id=\"$route.params.id\" />\n  </div>\n</template>\n\n<script>\nimport Answers from \"../components/Answers.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"view_Question\",\n  components: { Answers },\n  methods: {\n    check_cookie_value(name) {\n      var match = document.cookie.match(\n        new RegExp(\"(^| )\" + name + \"=([^;]+)\")\n      );\n      if (match) {\n        return match[2];\n      } else {\n        return -1;\n      }\n    },\n    answerQuestion() {\n      var id = this.check_cookie_value(\"id\");\n      var username = this.check_cookie_value(\"name\");\n      this.question.id = parseInt(this.question.id);\n      var day = new Date().toISOString().slice(0, 10).replace(/-/g, \"\");\n      axios\n        .post(\"http://localhost:8085/api/answers/create\", {\n          user: { id, username },\n          text: this.answer_txt,\n          correct: false,\n          query: {\n            id: this.question.id,\n            user: {\n              id: this.question.user.id,\n              username: this.question.user.username,\n            },\n            text: this.question.text,\n            date: this.question.date,\n            answers: this.question.answers,\n          },\n          date: day,\n        })\n        .then(function (response) {\n          console.log(response);\n        })\n        .catch((error) => {\n          if (error.response) {\n            // The request was made and the server responded with a status code\n            // that falls out of the range of 2xx\n            console.log(error.response.data);\n            console.log(error.response.status);\n            console.log(error.response.headers);\n          } else if (error.request) {\n            // The request was made but no response was received\n            // `error.request` is an instance of XMLHttpRequest in the browser and an instance of\n            // http.ClientRequest in node.js\n            console.log(error.request);\n          } else {\n            // Something happened in setting up the request that triggered an Error\n            console.log(\"Error\", error.message);\n          }\n        });\n    },\n  },\n  data() {\n    return {\n      id_number: this.$route.params.id,\n      question: [],\n      author: \"\",\n      dialog: false,\n      answer_txt: \"\",\n    };\n  },\n  mounted() {\n    var vm = this;\n    axios\n      .get(\"http://localhost:8085/api/queries/\" + vm.id_number)\n      .then(function (response) {\n        vm.question = response.data;\n        vm.author = vm.question.user.username;\n      })\n      .catch((error) => {\n        if (error.response) {\n          // The request was made and the server responded with a status code\n          // that falls out of the range of 2xx\n          console.log(error.response.data);\n          console.log(error.response.status);\n          console.log(error.response.headers);\n        } else if (error.request) {\n          // The request was made but no response was received\n          // `error.request` is an instance of XMLHttpRequest in the browser and an instance of\n          // http.ClientRequest in node.js\n          console.log(error.request);\n        } else {\n          // Something happened in setting up the request that triggered an Error\n          console.log(\"Error\", error.message);\n        }\n      });\n  },\n};\n</script>\n<style scoped>\n.Question {\n  height: 50vh;\n  width: 70vw;\n  margin-left: auto;\n  margin-right: auto;\n}\n.card-actions {\n  position: absolute;\n  bottom: 0;\n}\n</style>\n"],"sourceRoot":"src/views"}]}